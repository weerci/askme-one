	for (var i =3D 0; i < elements.length; i++)
	{
		var uniqueid =3D Lightboxes.length;
		var containerid =3D =
elements[i].getAttribute("rel").match(/Lightbox[_]?(\d*)?/).pop();
		containerid =3D (containerid ? containerid : 0);

		Lightboxes[uniqueid] =3D new vB_Lightbox(elements[i], uniqueid, =
